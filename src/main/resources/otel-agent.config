# Enable logging exporters
otel.traces.exporter=logging
otel.metrics.exporter=none
otel.logs.exporter=logging

# Disable SDK if needed (false by default, just showing it for completeness)
otel.sdk.disabled=false

# Resource attributes
otel.resource.attributes=service.name=unknown_service

# Attribute limits
otel.attribute.count.limit=128
otel.attribute.value.length.limit=

# Propagators
otel.propagators=tracecontext,baggage

# Span limits
otel.span.attribute.count.limit=128
otel.span.event.count.limit=128
otel.span.link.count.limit=128
otel.span.event.attribute.count.limit=128
otel.span.link.attribute.count.limit=128

# Log limits
otel.log.attribute.count.limit=128
otel.log.attribute.value.length.limit=

# Sampling
otel.traces.sampler=parentbased_always_on